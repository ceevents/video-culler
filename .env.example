# Environment Variables (optional)

# Analysis settings
FRAME_EXTRACTION_INTERVAL=1  # Extract 1 frame per N seconds
FOCUS_SCORE_THRESHOLD=70     # Threshold for "high score" selection

# Performance
MAX_PARALLEL_ANALYSIS=1      # Number of videos to analyze concurrently
THUMBNAIL_WIDTH=320          # Thumbnail width in pixels
THUMBNAIL_HEIGHT=180         # Thumbnail height in pixels

# Debug
DEBUG=false                  # Enable debug logging
DEV_TOOLS=false             # Open DevTools on startup
